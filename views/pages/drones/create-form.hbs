<h1>Create</h1>

<form action="/drones/create" method="post">
  <label for="name">Name: </label>
  <input type="text" name="name" id="name" required />
  <label for="propellers">Propellers: </label>
  <input type="number" name="propellers" id="propellers" required />
  <label for="maxSpeed">Speed: </label>
  <input type="number" name="maxSpeed" id="maxSpeed" required />
  <button type="submit">Create</button>
</form>