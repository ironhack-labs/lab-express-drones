<h2>Create a new drone</h2>

<form action="/drones/create" method="POST">
  <label>
    Name:
    <input type="text" name="name" required />
  </label>
  <br />
  <label>
    # of Propellers:
    <input type="number" name="propellers" required />
  </label>
  <br />
  <label>
    Max Speed:
    <input type="number" name="maxSpeed" required />
  </label>
  <br />
  <button> Save </button>
</form>