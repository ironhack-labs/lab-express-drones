<h2>Create a new drone</h2>

<form action="/drones/create" method="POST">
    <label >
        Nombre
        <input type="text" name="name">
    </label>
    <label>
        Propulsores
        <input type="number" name="propellers">
    </label>
    <label>
        Velocidad maxima
        <input type="number" name="maxSpeed">
    </label>
    <input type="submit" value="crear dron">

</form>