<h2>Create a new drone</h2>

<form method="POST" action="/drones/create">
  <label>
    Name
    <input type="text" name="name" />
  </label>

  <label>
    Propellers
    <input type="text" name="propellers" />
  </label>

  <label>
    Max. Speed
    <input type="text" name="maxSpeed" />
  </label>

  <button type="submit">Create</button>
</form>