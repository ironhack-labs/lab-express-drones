<h2>Create a new drone</h2>


<form action="/drones/create" method="post">
 
  <label for="title-input">Name:</label>
  <input type="text" name="name" id="title-input" />
 
  <label for="propellers-input">Propellers:</label>
  <input type="text" name="propellers" id="propellers-input" />
 
  <label for="speed-input">MaxSpeed:</label>
  <input type="number" name="maxSpeed" id="speed-input" min="1" max="30" />
 
  <button type="submit">Create</button>
 
</form>