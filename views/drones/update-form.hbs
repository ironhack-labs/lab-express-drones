<h2>Update the drone</h2>

<form action="/drones/create" method="post">

<label for="name">Name</label>
<input type="text" name="name" id=" name" value="{{name}}" />

<label for="propellers">Propeller</label>
<input type="number" name="propellers" id=" propellers" value="{{propellers}}" />

<label for="maxSpeed">Speed</label>
<input type="number" name="maxSpeed" id="maxSpeed" value="{{maxSpeed}}" />

<button type="submit">Update</button>

</form>