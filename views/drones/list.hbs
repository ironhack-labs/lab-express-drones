<h2>Available drones</h2>

{{#each drones}}
    <div> 
        <a>
            <p>Name:  {{ this.name }}</p>
        </a> 
        <a>
            <p>Propellers:  {{ this.propellers }}</p>
        </a> 
        <a>
            <p>maxSpeed:  {{ this.maxSpeed }}</p>
        </a> 
    </div>
    <br><br>
    <div>
        <a href="/drones/{{ this._id }}/edit"> Editing </a>
        <form method="POST" action="/drones/{{this._id}}/delete">
            <button type="submit"> Delete </button>
        </form>
    </div>
{{/each}}