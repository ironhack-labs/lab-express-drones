<h2>Available drones</h2>
{{#each drones}}

    <p>{{this.name}}</p>

<p>{{this.propellers}}</p>
<p>{{this.maxSpeed}}</p>
<a href="/drones/{{this._id}}/edit">
    <button type="button" class="btn btn-dark">Edit</button>
</a>
<form action="/drones/{{this._id}}/delete" method="POST">
    <button type="submit" class="btn btn-dark">Delete</button>
</form>
{{/each}}