<h2>Available drones</h2>

{{#each drones}}
<section>
    <p> 
        Name: {{name}}<br>
        Propellers: {{Propellers}}<br>
        Speed: {{maxSpeed}}<br>
        <a href="/drones/{{this.id}}/edit">edit</a>
        </p>
        <form action="/drones/ {{_id}}/delete" method="POST"
            <button>Delete</button>
            </form>

</section>
{{/each}}