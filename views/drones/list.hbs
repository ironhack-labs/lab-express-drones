<div class="container mt-5">
  <h2>Available drones</h2>
  <hr>
  {{#each foundDrones}}
  <p><strong>Name: </strong>{{name}}</p>
  <p><strong>Propellers: </strong>{{propellers}}</p>
  <p><strong>Max. Speed: </strong>{{maxSpeed}}</p>
  <a href="/drones/{{_id}}/edit" class="btn btn-dark">Update</a>
  <form action="/drones/{{_id}}/delete" method="POST">
    <input type="submit" value="Eliminar" class="btn btn-dark" />
  </form>
  <hr />
  {{/each}}
</div>