<h2>Available drones</h2>

{{#each drones}}
<div>
    <h3>{{name}}</h3>
    <p>Propeller: {{propellers}}</p>
    <p>Speed: {{maxSpeed}}</p>
     <a href="/drones/{{_id}}/edit">Edit</a> 

     <form action="/drones/{{id}}/delete" method="post">
<button type="submit">Delete drone</button>
    </form>
</div>
{{/each}}