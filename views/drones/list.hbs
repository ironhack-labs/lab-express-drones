<h2>Available drones</h2>
<div class="list">

    {{#each drones}}

    <p>{{name}}</p>

    <p>{{propellers}}</p>
    <p>{{maxSpeed}}</p>
    <a class="btn btn-info" href="/drones/{{id}}/edit">Actualizar Drone</a>


    <form action="/drones/{{id}}/delete" method="POST">
        <input type="submit" value="eliminar">
    </form>


    {{/each}}

</div>