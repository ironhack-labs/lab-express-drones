<div>
  <h2 class="title">List of available drones</h2>
  <hr>
  <div class="drones">
    {{#each DronesFromDB}}
    <div class="dronebox">
      <h3>{{name}}</h3>
      <div class="middledronebox">Number of propellers: {{propellers}}. </div>
      <div>Max. speed: {{maxSpeed}} m per second.</div>
    </div>
    {{/each}}
  </div>
  <hr>
</div>

