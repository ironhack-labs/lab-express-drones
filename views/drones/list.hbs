<h2>Available drones</h2>
{{#each this as |drone|}}
{{#if this.id}}
    <div>{{drone.name}}</div>
    <div>{{drone.propellers}}</div>
    <div>{{drone.maxSpeed}}</div>
    <span>
        <a href='/drones/{{id}}/edit'>Edit Drone</a>
        <a href='/drones/{{id}}/delete'>Delete Drone</a>
    </span>
{{/if}}
{{/each}}