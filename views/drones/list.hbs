<h2>Available drones</h2>
<hr />

{{#each drones}}
  <h3>Name: {{name}}</h3>
  <br />
  <p>Propellers: {{propellers}}</p>
  <p>Speed: {{maxSpeed}}</p>
  <br />
  <a href='/drones/{{_id}}/edit'>Edit</a>
  <form action='/drones/{{_id}}/delete' method='POST'>
    <input type='submit' value='Delete' />
  </form>
  <hr />
{{/each}}