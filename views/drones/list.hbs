<h2>Available drones</h2>


{{#each drones}}
    <div class="drone">
   {{this.name}}
    {{this.propellers}}
    {{this.speed}}  
    <a href="/drones/{{this._id}}/edit" ><button type="button" class="btn btn-dark">Edit</button></a>
    <a href="/drones/{{this._id}}/delete" ><button type="button" class="btn btn-dark">Delete</button></a>
    </div>
{{/each}}


