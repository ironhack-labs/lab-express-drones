<h2>Available drones</h2>
<ul class="list-group">
    {{#each drones}}
    <li class="list-group-item">
        <h3>Name:{{name}} <br>
            Propellers:{{propellers}} <br>
            Max Speed:{{maxSpeed}} <br></h3>
        <a class="btn btn-secondary" href="/drones/{{_id}}/edit">Edit</a>
        <form action="/drones/{{_id}}/delete" method="POST">
            <button class="btn btn-danger">Delete</button>
        </form>
    </li>
    {{/each}}
</ul>