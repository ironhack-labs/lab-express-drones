<h2>Available drones</h2>

{{#each drones}}
<h3>{{this.name}}</h3>
<p>Propellers: {{this.propellers}}</p>
<p>Max Speed: {{this.maxSpeed}}</p>
<a href="/drones/{{this.id}}/edit">Update Drone</a>
<form method="POST" action="/drones/{{this.id}}/delete">
    <button type="submit">Delete</button>
</form>
{{/each}}



