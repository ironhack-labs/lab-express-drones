<h2>Available drones</h2>

<a href="/drones/create" class="btn btn-primary" role="button">Create new drone</a>

{{#each drones}}
  <h1>name: {{this.name}}</h1>
  <h2>propellers: {{this.propellers}}</h2>
  <h3>maxSpeed: {{this.maxSpeed}}</h3>
  <a href="/drones/{{id}}" class="btn btn-primary" role="button">See more</a>
{{/each}}