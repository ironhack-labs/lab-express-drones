<h2>Available drones</h2>
<ul>
  {{#each drones}}
    <li>Nombre: {{name}}</li>
    <li>Helices: {{propellers}}</li>
    <li>Velocidad maxima:{{maxSpeed}}</li>
    <a href="/drones/{{_id}}/edit">Editar dron</a>
    <br />
    <form action="/drones/{{_id}}/delete" method="POST">
      <button>Eliminar dron</button>
    </form>

    <hr />
  {{/each}}

</ul>