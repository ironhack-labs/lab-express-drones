<h2>Available drones</h2>

{{#each drones}}

    <h2>{{this.name}}</h2>
    <h3>{{this.propellers}}</h3>
    <h3>{{this.maxSpeed}}</h3>

    <a href="/drones/{{this._id}}/edit">Edit</a>
    <form action="/drones/{{this._id}}/delete" method="POST"> <button type="submit">Delete</button></form>

{{/each}}



