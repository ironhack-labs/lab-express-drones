<h1>Available drones</h1>
<hr>
<div class="container">
    <div class="row">
        {{#each drones }}
        <p> {{name}} </p>
        <p> {{propellers}} </p>
        <p> {{maxSpeed}} </p>
        <a href="/drones/{{_id}}/edit" class="btn btn-warning"> Edit </a>
        <form action="/drones/{{_id}}/delete" method="post">
            <input type="submit" value="eliminar" class="btn btn-danger">
        </form>
        {{/each}}
    </div>
</div>