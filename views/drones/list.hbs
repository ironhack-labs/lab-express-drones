
<section id="list-container">
    <h2>Available drones</h2>
    <hr>
    {{#each drones}}
        <div id="done-info">
            <p> <b> Name:</b> {{name}} </p>
            <p> <b> Propellers:</b> {{propellers}} </p>
            <p> <b> Max-speed:</b> {{maxSpeed}} </p>
        </div>
        <a href="/drones/{{_id}}/edit">Edit</a>
        <hr>
    {{/each}}
    
</section>
