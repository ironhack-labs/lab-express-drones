<h2>Available drones</h2>

{{#each dronesFromDB}}
<section>
    <h3>{{this.name}}</h3>
    <p>{{this.propellers}}</p>
    <p>{{this.maxSpeed}}</p>
    <a href="/drones/{{this.id}}/edit"> Edit drone details</a>
</section>
    <form action="/drones/{{this._id}}/delete" method="POST">
        <button>Delete</button>
    </form>
{{/each}}