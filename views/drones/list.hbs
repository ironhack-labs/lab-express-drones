<h1>Available drones</h2>
<hr>
{{#each drones}}
<h2>Name:  {{this.name}}</h3>
<hr>
<h4>Number of propellers:  {{this.propellers}}</h4>
<hr>
<h4>Top Speed:  {{this.maxSpeed}}</h4>
<hr>
<a href="/drones/{{this._id}}/edit"><button>Edit</button></a> 
<hr>
<form action="/drones/{{_id}}/delete" method="POST">
      <button>Delete</button>
<hr>
<hr>
{{/each}}